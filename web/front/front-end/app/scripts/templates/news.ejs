<div class="panel panel-default">
    <div class="panel-heading text-center" style="background-color: #2A0247; color: white;">
    	<%= Title %>
    </div>
    <div class="panel-body" style="background-color: #54038F; color: white;">
        <div class="row">
            <div class="col-md-12 text-center">
                <img src="<%= Image.url %>" width="40%">
            </div>
        </div>
        <div class="row">
        	<div>
    			<%= Text %>
    		</div>
    	</div>
    </div>
</div>